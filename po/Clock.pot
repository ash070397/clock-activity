"<markup><span lang=\"en\" font_desc=\"Sans 20\"><span foreground=\"#B20008\">"
"%A</span>, <span foreground=\"#5E008C\">%m</span>/<span foreground="
"\"#B20008\">%d</span>/<span foreground=\"#9A5200\">%Y</span></span></markup>"
msgstr ""

